public class Main {
  public static void main (String[] args){
    int[][] numbers = {
      {10, 20},
      {30, 40},
      {50, 60}
    };

    System.out.println(numbers[1][0]);

    for (int i =0; i < numbers.length; i++){
      for (int j =0; j < numbers[i].length; j++){
        System.out.print(numbers[i][j] + " ");
      }
      System.out.println();
    }
  }
}













