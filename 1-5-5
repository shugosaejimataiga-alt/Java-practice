public class Main {
  public static void main(String[] args) {
    int[][] numbers = {
      {5, 2, 9},
      {1, 7, 3},
      {8, 6, 4}
    };

    int max = numbers[0][0]; // æœ€åˆã®è¦ç´ ã§åˆæœŸåŒ–
    int min = numbers[0][0];

    for (int i = 0; i < numbers.length; i++) {
      for (int j = 0; j < numbers[i].length; j++) {
        if (numbers[i][j] > max) {
          max = numbers[i][j];
        }
        if (numbers[i][j] < min) {
          min = numbers[i][j];
        }
      }
    }

    System.out.println("æœ€å¤§å€¤: " + max);
    System.out.println("æœ€å°å€¤: " + min);
  }
}

