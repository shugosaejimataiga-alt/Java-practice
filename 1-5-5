public class Main {
  public static void main (String[] args){
    int[][] numbers ={
      {1, 2, 3},
      {4, 5, 6}
    };

    System.out.println(numbers[0][2]);

    int sum = 0;

    for (int i =0; i < numbers.length; i++){
      for (int j =0; j < numbers[i].length; j++){
        sum += numbers[i][j];
        System.out.println(numbers[i][j] + "| åˆè¨ˆ:" + sum);
      }
    }
  }
}
