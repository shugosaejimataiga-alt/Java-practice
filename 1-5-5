import java.util.Arrays;

public class Main {
  public static void main(String[] args) {
    int[][] numbers = {
      {12, 7, 9},
      {4, 15, 6},
      {8, 2, 10}
    };

    // 1. å¤šæ¬¡å…ƒé…åˆ—ã‚’ä¸€æ¬¡å…ƒã«å¤‰æ›
    int totalElements = numbers.length * numbers[0].length;
    int[] flat = new int[totalElements];
    int index = 0;

    for (int i = 0; i < numbers.length; i++) {
      for (int j = 0; j < numbers[i].length; j++) {
        flat[index] = numbers[i][j];
        index++;
      }
    }

    // 2. ä¸€æ¬¡å…ƒé…åˆ—ã‚’ã‚½ãƒ¼ãƒˆ
    Arrays.sort(flat);

    // 3. ã‚½ãƒ¼ãƒˆçµæœã‚’è¡¨ç¤º
    System.out.println("ã‚½ãƒ¼ãƒˆçµæœ:");
    for (int n : flat) {
      System.out.print(n + " ");
    }
  }
}

