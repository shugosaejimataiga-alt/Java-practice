public class Main {
  public static void main {
    int[][] numbers = {
      {12, 7, 9},
      {4, 15, 6},
      {8, 2, 10}
    };
    
    int max = numbers[0][0];
    int min = numbers[0][0];

    for (int i =0; i < numbers.length; i++){
      for (int j =0; j < numbers[i].length; j++){
        if (numbers[i][j] > max){
          max = numbers[i][j];
        }
        if (numbers[i][j] < min){
          min = numbers[i][j];
        }
      }
    }
    System.out.println("æœ€å¤§å€¤ã¯" + max);
    System.out.println("æœ€å°å€¤ã¯" + min);
  }
}

